e="35"/><column index="2" value="207"/><column index="3" value="300"/><column index="4" value="154"/><column index="5" value="97"/><column index="6" value="57"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="WorkoutTypes" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="35"/><column index="2" value="95"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">-- Önce tekrar eden kayıtları kontrol edelim
WITH DuplicateCheck AS (
    SELECT partId, exerciseId, orderIndex,
           ROW_NUMBER() OVER (
               PARTITION BY partId, exerciseId 
               ORDER BY orderIndex DESC
           ) as rn
    FROM PartExercises
)
SELECT * FROM DuplicateCheck 
WHERE rn &gt; 1;

-- Tekrar eden kayıtları silelim
DELETE FROM PartExercises
WHERE rowid NOT IN (
    SELECT MIN(rowid)
    FROM PartExercises
    GROUP BY partId, exerciseId
);

-- OrderIndex'leri düzenleyelim
-- Geçici tablo oluştur
CREATE TEMPORARY TABLE temp_part_exercises AS
SELECT partId, exerciseId,
    (ROW_NUMBER() OVER (
        PARTITION BY partId 
        ORDER BY orderIndex
    ) * 10) as new_order
FROM PartExercises;

-- Ana tabloyu güncelle
UPDATE PartExercises
SET orderIndex = (
    SELECT new_order 
    FROM temp_part_exercises 
    WHERE temp_part_exercises.partId = PartExercises.partId 
    AND temp_part_exercises.exerciseId = PartExercises.exerciseId
);

-- Geçici tabloyu sil
DROP TABLE temp_part_exercises;</sql><current_tab id="0"/></tab_sql></sqlb_project>
